{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "kiezjagd.de" }
      ],
      "destination": "https://www.kiezjagd.de/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "kiezjagd.vercel.app" }
      ],
      "destination": "https://www.kiezjagd.de/$1",
      "permanent": true
    }
  ]
}
